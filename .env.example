# Claude AI — required for image and scanned-PDF text extraction
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# ElevenLabs — required for text-to-speech generation
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Paid.ai — billing and quota enforcement
# Leave PAID_API_URL blank (or omit it) to run in stub mode:
# all requests are allowed and usage is only logged locally.
PAID_API_URL=https://api.agentpaid.io/api/v1
PAID_API_KEY=your_paid_api_key_here
# The externalId of the customer you created in your Paid.ai dashboard.
# Defaults to "demo-company-1" if not set.
PAID_COMPANY_ID=demo-company-1
# Set to "false" in production to hard-block requests when billing is unreachable.
# Defaults to "true" (fail-open) so dev environments are never blocked.
PAID_FAIL_OPEN=true

# LangSmith — optional, for tracing
LANGSMITH_API_KEY=your_langsmith_api_key_here
